<p id="notice"><%= notice %></p>

<h1>Word Nets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @word_nets.each do |word_net| %>
      <tr>
        <td><%= link_to 'Show', word_net %></td>
        <td><%= link_to 'Edit', edit_word_net_path(word_net) %></td>
        <td><%= link_to 'Destroy', word_net, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Word Net', new_word_net_path %>
